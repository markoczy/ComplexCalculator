package mkz.cc.test;

import mkz.cc.core.debug.CCalcException;
import mkz.cc.core.equation.Constant;
import mkz.cc.core.equation.Operation;
import mkz.cc.core.equation.linear.LinearEquation;
import mkz.cc.util.IO;

public class TestLinearEquation
{

	public static void main(String[] args)
	{
		// 5 - 1 + 4 / 2 * 5
		
		LinearEquation lEq = new LinearEquation();
		lEq.addOperation(new Constant(5), Operation.SUB);
		lEq.addOperation(new Constant(1), Operation.ADD);
		lEq.addOperation(new Constant(4), Operation.DIV);
		lEq.addOperation(new Constant(2), Operation.MPL);
		lEq.addOperation(new Constant(5), Operation.UNDEF);
		
		try
		{
			System.out.println("Value is: "+lEq.getValue());
		}
		catch (CCalcException e)
		{
			IO.SysOutE(e);
		}
		
		
	}

}
